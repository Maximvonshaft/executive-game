{
  "name": "executive-game",
  "version": "0.1.0",
  "description": "Telegram Mini App foundations for Practice Card Games",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node src/server.js",
    "test": "node --test \"test/**/*.test.js\"",
    "test:backend": "node --test \"test/**/*.test.js\"",
    "test:client": "echo '前端单测需在具备 Vitest 的环境执行'",
    "lint": "node scripts/lint.js",
